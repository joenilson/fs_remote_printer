{
  "name": "fs_remote_printer",
  "version": "1.0.0",
  "description": "FacturaScripts Remote Printer",
  "main": "index.js",
  "scripts": {
    "start": "electron index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joenilson/fs_remote_printer.git"
  },
  "author": "FacturaScripts",
  "license": "LGPL-3.0",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "electron": "^1.6.10",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "node-notifier": "^5.1.2",
    "node-pre-gyp": "^0.6.36",
    "node-printer": "^1.0.2",
    "printer": "git+https://github.com/tojocky/node-printer.git"
  },
  "build": {
    "appId": "com.facturascripts.remote_printer",
    "nodeGypRebuild": true,
    "mac": {
      "category": "public.app-category.utilities"
    },
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "joenilson",
        "repo": "fs_remote_printer"
      }
    ]
  },
  "devDependencies": {
    "electron-builder": "19.19.1"
  }
}
